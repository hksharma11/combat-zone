<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:paddingStart="5sp"
    android:background="@drawable/walletcorner"
    android:paddingEnd="5sp"
    android:elevation="2dp"
    android:paddingBottom="15sp"
    android:paddingTop="5sp"
    android:layout_marginBottom="15sp"
    android:backgroundTint="#fff"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:ignore="RtlSymmetry">
    
    
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="90dp"
            android:id="@+id/image"
            android:layout_height="90dp"
            android:src="@drawable/podium"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_toEndOf="@id/image"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_height="90dp">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
               android:gravity="center"
                android:textSize="22sp"
                android:id="@+id/winnerslist_textview_gamename"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textSize="15sp"

                android:gravity="center"
                android:id="@+id/winnerslist_textview_date_time"

                />




        </LinearLayout>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:background="#FBA019"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rank"
            android:textColor="#fff"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginStart="10sp"
            android:layout_alignParentStart="true"
            android:id="@+id/rank"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="25sp"
            android:textSize="18sp"
            android:textColor="#fff"
            android:textStyle="bold"
            android:layout_toEndOf="@id/rank"
            android:text="Name"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Price"
            android:textSize="18sp"
            android:textColor="#fff"
            android:textStyle="bold"
            android:layout_marginEnd="25sp"
            android:layout_alignParentEnd="true"
            />


    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/winnerslist_textview_row1"
        android:layout_marginTop="5dp"
        android:visibility="gone"
        android:layout_height="wrap_content">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9"

            android:textColor="#000"
            android:textSize="18sp"

            android:layout_marginStart="25sp"
            android:layout_alignParentStart="true"
            android:id="@+id/winnerslist_textview_row1_rank"/>


        <TextView
