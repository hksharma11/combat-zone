            android:textSize="18sp"
            android:textColor="#000"
            android:id="@+id/winnerslist_textview_row1_name"
            android:layout_toEndOf="@id/winnerslist_textview_row1_rank"
            android:text="Harsh Sharma"/>


        <TextView
